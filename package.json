{
  "name": "print-ui-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "docker-build": "vue-tsc && vite build --base=/ui",
    "preview": "vite preview",
    "lint": "eslint --ext ts,vue src/ --fix",
